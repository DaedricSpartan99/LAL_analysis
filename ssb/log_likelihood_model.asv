function logL = log_likelihood_model(X, Parameters)

    % compute discrepancy
    disc = var(Parameters.y);
    Y = uq_evalModel(Parameters.ForwardModel, X);
    N = size(X,1);

    % TODO: vectorize
    logL = zeros(N);
    for i = 1:N
        logL(i) = sum(-(Parameters.y - Y(i))^2/ disc /2. - 0.5*log(pi*disc));
    end
end